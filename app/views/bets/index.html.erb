<p id="notice"><%= notice %></p>


<div class="container">
  <div class="d-flex align-items-left">
    <%= render "score_jumbo", users: @users%>
  </div>
  <div class="d-flex align-items-center p-3 my-3 text-white-50 rounded box-shadow" style="background-color: #1f993f;">
      <%= image_tag("logo_planvital.png", class: "mr-3", style:'height:85px; width:auto;') %>
      <div class="lh-100">
          <h4 class="mb-0 text-white lh-100" >Apuestas</h4>
      </div>
  </div>
  <% @matches.each do |match| %>
    <%= render 'bet_jumbo', match: match, bets: @bets %>
  <% end %>
</div>
